{
  "expo": {

    "name": "roverview",
    "slug": "roverview",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/system/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/images/system/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },

    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/system/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
